@model  IEnumerable<string>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>
    @section styles{
        <link href="~/Content/Custom_Global.css" rel="stylesheet" />
    }
</head>


<body>
    <div class="container">
        <div class="col-lg-12">
            <a href="@Url.Action("ReportToPDF_Ver2")" class="btn btn-info">Download To PDF</a>
        </div>
        <div class="col-lg-12">
            <h4>This is Reportviewer to image ver2</h4>
        </div>
        @foreach (var image in Model)
        {
            <div class="col-lg-12 text-center">
                <img src="@image" style="width: 100%;" />
            </div>
        }
    </div>
</body>